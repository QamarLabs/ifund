networks:
  fund-islam-net:
    name: fund-islam-net
services:
  common:
    build:
      context: C:\Users\17275\pocs\fund-islam
      dockerfile: /common/Dockerfile
    dns:
    - 8.8.8.8
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: aliqamarlabs/fund-islam-common
    networks:
      fund-islam-net: {}
    ports:
    - target: 80
    - target: 443
  initiatives:
    build:
      context: C:\Users\17275\pocs\fund-islam
      dockerfile: /initiatives/Dockerfile
    dns:
    - 8.8.8.8
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: http://+:80
    image: aliqamarlabs/fund-islam-initiatives
    networks:
      fund-islam-net: {}
    ports:
    - target: 80
  local-service:
    build:
      args:
        BUILD_CONFIGURATION: Debug
      context: C:\Users\17275\pocs\fund-islam
      dockerfile: local-service/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: local-service
      target: base
    container_name: local-service
    dns:
    - 8.8.8.8
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /.nuget/fallbackpackages;/.nuget/fallbackpackages2
    image: aliqamarlabs/fund-islam-localservice:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /.nuget/packages
        --additionalProbingPath /.nuget/fallbackpackages --additionalProbingPath /.nuget/fallbackpackages2  "/app/bin/Debug/net7.0/local-service.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      fund-islam-net: {}
    ports:
    - published: 8008
      target: 80
    tty: true
    volumes:
    - C:\Program Files (x86)\Microsoft Visual Studio\Shared\NuGetPackages:/.nuget/fallbackpackages:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/.nuget/fallbackpackages2:ro
    - C:\Users\17275\.nuget\packages:/.nuget/packages:ro
    - C:\Program Files\Microsoft Visual Studio\2022\Community\Common7\IDE\CommonExtensions\Microsoft\HotReload:/HotReloadAgent:ro
    - C:\Program Files\Microsoft Visual Studio\2022\Community\MSBuild\Sdks\Microsoft.Docker.Sdk\tools\HotReloadProxy\linux-x64\net7.0:/HotReloadProxy:ro
    - C:\Program Files\Microsoft Visual Studio\2022\Community\MSBuild\Sdks\Microsoft.Docker.Sdk\tools\TokenService.Proxy\linux-x64\net7.0:/TokenService.Proxy:ro
    - C:\Users\17275\pocs\fund-islam\local-service:/app:rw
    - C:\Users\17275\AppData\Roaming\ASP.NET\Https:/home/app/.aspnet/https:ro
    - C:\Users\17275\AppData\Roaming\Microsoft\UserSecrets:/home/app/.microsoft/usersecrets:ro
    - C:\Users\17275\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\17275\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\17275\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\17275\pocs\fund-islam:/src:rw
  redis:
    dns:
    - 8.8.8.8
    image: redis:6
    networks:
      fund-islam-net: {}
    ports:
    - published: 6379
      target: 6379
  startup-businesses:
    build:
      context: C:\Users\17275\pocs\fund-islam
      dockerfile: /startup-businesses/Dockerfile
    dns:
    - 8.8.8.8
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: http://+:80
    image: aliqamarlabs/fund-islam-startup-businesses
    networks:
      fund-islam-net: {}
    ports:
    - target: 80
  users:
    build:
      context: C:\Users\17275\pocs\fund-islam
      dockerfile: /users/Dockerfile
    dns:
    - 8.8.8.8
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: http://+:80
    image: aliqamarlabs/fund-islam-users
    networks:
      fund-islam-net: {}
    ports:
    - target: 80
version: '3.4'